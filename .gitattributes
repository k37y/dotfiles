.gitattributes !filter !diff
.ssh/config filter=git-crypt diff=git-crypt
.ssh/id_rsa filter=git-crypt diff=git-crypt
.ssh/id_rsa.pub filter=git-crypt diff=git-crypt
.ssh/known_hosts filter=git-crypt diff=git-crypt
.ssh/authorized_keys filter=git-crypt diff=git-crypt
.dnsmasq.d/dhcp.conf filter=git-crypt diff=git-crypt
.dnsmasq.d/dns.conf filter=git-crypt diff=git-crypt
.hosts filter=git-crypt diff=git-crypt
